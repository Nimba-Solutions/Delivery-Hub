        <modifyAllRecords>false</modifyAllRecords>
        <object>NetworkEntity__c</object>
        <viewAllRecords>false</viewAllRecords>
    </objectPermissions>
    <pageAccesses>
        <apexPage>DeliveryStatusPage</apexPage>
        <enabled>true</enabled>
    </pageAccesses>
</PermissionSet>
