    </fieldPermissions>
    <fieldPermissions>
        <editable>false</editable>
        <field>WorkItem__c.ClientNetworkEntityId__c</field>
        <readable>true</readable>
    </fieldPermissions>
    <fieldPermissions>
        <editable>false</editable>
        <field>WorkItem__c.WorkflowTypeTxt__c</field>
