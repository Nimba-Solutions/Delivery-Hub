    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Picklist</type>
    <valueSet>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>Will Do</fullName>
                <default>false</default>
                <label>Will Do</label>
            </value>
            <value>
                <fullName>Sizing Only</fullName>
                <default>false</default>
                <label>Sizing Only</label>
            </value>
            <value>
                <fullName>Deferred</fullName>
                <default>false</default>
                <label>Deferred</label>
            </value>
            <value>
                <fullName>On Hold</fullName>
                <default>false</default>
                <label>On Hold</label>
            </value>
        </valueSetDefinition>
    </valueSet>
</CustomField>
