<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>BudgetVarianceNumber__c</fullName>
    <description>(Positive = Under Budget, Negative = Over Budget)</description>
    <inlineHelpText>Difference between pre-approved hours and total logged hours. Negative means over budget.</inlineHelpText>
    <externalId>false</externalId>
    <formula>EstimatedHoursNumber__c - TotalLoggedHoursNumber__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Budget Variance</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
