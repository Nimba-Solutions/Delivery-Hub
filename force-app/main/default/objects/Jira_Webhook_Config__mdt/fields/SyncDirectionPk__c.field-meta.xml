<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SyncDirectionPk__c</fullName>
    <description>The direction of synchronization for this event type. Controls whether changes flow from Jira to Salesforce, Salesforce to Jira, or both directions.</description>
    <fieldManageability>DeveloperControlled</fieldManageability>
    <label>Sync Direction</label>
    <required>true</required>
    <type>Picklist</type>
    <valueSet>
        <restricted>true</restricted>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>Jira_to_Salesforce</fullName>
                <default>true</default>
                <label>Jira to Salesforce</label>
            </value>
            <value>
                <fullName>Salesforce_to_Jira</fullName>
                <default>false</default>
                <label>Salesforce to Jira</label>
            </value>
            <value>
                <fullName>Bidirectional</fullName>
                <default>false</default>
                <label>Bidirectional</label>
            </value>
        </valueSetDefinition>
    </valueSet>
</CustomField>
